File f = new File("/tmp/twitlogic-full.trig.gz");
Sail sail = new MemoryStore();
sail.initialize();
try {
    SailLoader loader = new SailLoader(sail);
    loader.setBufferSize(1000);  // this is the default
    loader.setVerbose(true);
    loader.load(f);
} finally {
    sail.shutDown();
}